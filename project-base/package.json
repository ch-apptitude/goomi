{
  "name": "{{name}}",
  "version": "0.0.1",
  "description": "A react boilerplate with redux, saga based on a feature-oriented structure",
  "main": "./src/server/server.babel.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config ./webpack/webpack.config.devnossr.js ",
    "dev": "cross-env NODE_ENV=development node ./src/server/server.babel.js",
    "dev-nossr": "cross-env NODE_ENV=development node ./src/server/server.babel.js",
    "prod": "cross-env NODE_ENV=production node ./src/server/server.babel.js",
    "prodServer": "PORT=5051 NODE_ENV=production forever start -l '{{name}}-prod.log' -a ./src/server/server.babel.js",
    "stageServer": "PORT=5061 NODE_ENV=production forever start -l '{{name}}-stage.log' -a ./src/server/server.babel.js",
    "devServer": "PORT=5071 NODE_ENV=production forever start -l '{{name}}-dev.log' -a ./src/server/server.babel.js",
    "build": "npm run clean && npm run build:client && npm run build:server",
    "build:server": "cross-env NODE_ENV=production webpack --config ./webpack/server.babel.js",
    "build:client": "cross-env NODE_ENV=production webpack --config ./webpack/prod.babel.js",
    "generate": "./node_modules/.bin/plop --plopfile ./generators/index.js",
    "clean": "rm -rf build/*",
    "extract-intl": "babel-node tools/run extractMessages"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "es6-promise": "^4.1.0",
